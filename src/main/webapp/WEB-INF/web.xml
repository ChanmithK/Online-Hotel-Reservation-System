<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">



<servlet>
<servlet-name>login</servlet-name>
<servlet-class>com.reservation.ManagerLoginServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>login</servlet-name>
<url-pattern>/log</url-pattern>
</servlet-mapping>

 
<servlet>
<servlet-name>cusInsert</servlet-name>
<servlet-class>com.reservation.CusInsert</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>cusInsert</servlet-name>
<url-pattern>/insert</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>Update Reservation</servlet-name>
<servlet-class>com.reservation.UpdateReservationServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>Update Reservation</servlet-name>
<url-pattern>/update</url-pattern>
</servlet-mapping> 


<servlet>
<servlet-name>Delete Reservation</servlet-name>
<servlet-class>com.reservation.DeleteReservationServlet</servlet-class>
</servlet>


<servlet-mapping>
<servlet-name>Delete Reservation</servlet-name>
<url-pattern>/delete</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>SearchCustomer</servlet-name>
<servlet-class>com.reservation.SearchDataServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>SearchCustomer</servlet-name>
<url-pattern>/search</url-pattern>
</servlet-mapping>

</web-app>